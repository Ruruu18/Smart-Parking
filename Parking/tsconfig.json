{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "baseUrl": ".",
    "paths": {},
    "allowJs": true,
    "checkJs": false,
    "noEmit": true
  },
  "include": ["src/**/*", "index.html", "vite.config.ts", "vite.config.js"],
  "exclude": ["node_modules", "dist"]
}
